<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="graduationsurcas.com.graduationapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activites.MainActivity"
            android:label="@string/title_placeslist" >
        </activity>
        <activity
            android:name=".activites.placeinformation"
            android:label="@string/title_activity_placeinformation" >
        </activity>
        <activity
            android:name=".activites.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.items_list"
            android:label="@string/title_activity_items_list" >
        </activity>
    </application>

</manifest>
